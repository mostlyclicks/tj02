<ul>
  <% if ::Refinery::Graphs::Admin::GraphsController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.graphs_admin_graphs_path %>
    </li>
  <% end %>
  <li>
    <%= link_to t('.create_new'), refinery.new_graphs_admin_graph_path,
                 :class => "add_icon" %>
  </li>
<% if !searching? && ::Refinery::Graphs::Admin::GraphsController.sortable? && ::Refinery::Graphs::Graph.many? %>
  <li>
    <%= link_to t('.reorder', :what => "Graphs"),
                 refinery.graphs_admin_graphs_path,
                 :id => "reorder_action",
                 :class => "reorder_icon" %>

    <%= link_to t('.reorder_done', :what => "Graphs"),
                 refinery.graphs_admin_graphs_path,
                 :id => "reorder_action_done",
                 :style => "display: none;",
                 :class => "reorder_icon" %>
  </li>
<% end %>
</ul>
