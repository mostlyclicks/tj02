

<div class="container">
  <h1>Graphs</h1>
  <div class="row">
    
    <% @graphs.each_with_index do |g, index| %>

      <div class="col-lg-6 graph">
     <h5><%= link_to g.name, refinery.graphs_graph_path(g.id) %></h5>
      <%#= raw g.excerpt %>
      <div><%= raw g.code %></div>
      </div>
    <% end %>
  </div>
</div>